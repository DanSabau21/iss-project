<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.iss_ltbtp.Presentation.Controller.TesterMainController"
            prefHeight="400.0" prefWidth="600.0">
    <Label layoutX="178.0" layoutY="14.0" text="Tester's Main Page" textAlignment="CENTER">
        <font>
            <Font size="24.0" />
        </font>
    </Label>

    <ListView fx:id="bugListView" layoutX="24.0" layoutY="62.0" prefHeight="286.0" prefWidth="551.0" />
    <Button layoutX="24.0" layoutY="361.0" mnemonicParsing="false" onAction="#logout" text="Logout" textAlignment="CENTER" />

    <TextField fx:id="descriptionField" layoutX="128.0" layoutY="361.0" promptText="Description" />
    <Button layoutX="326.0" layoutY="361.0" mnemonicParsing="false" onAction="#addBug" text="Add Bug" textAlignment="CENTER" />
</AnchorPane>
